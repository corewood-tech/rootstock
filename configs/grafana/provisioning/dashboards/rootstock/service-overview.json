{
  "uid": "rootstock-overview",
  "title": "Rootstock â€” Service Overview",
  "tags": ["rootstock", "otel"],
  "timezone": "browser",
  "editable": true,
  "refresh": "10s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "templating": {
    "list": []
  },
  "panels": [
    {
      "id": 1,
      "title": "Request Rate (spans/s)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "datasource": { "uid": "prometheus", "type": "prometheus" },
      "targets": [
        {
          "expr": "sum(rate(span_metrics_calls_total{service_name=\"rootstock\"}[1m]))",
          "legendFormat": "req/s"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 15,
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "auto"
          }
        },
        "overrides": []
      }
    },
    {
      "id": 2,
      "title": "Error Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "datasource": { "uid": "prometheus", "type": "prometheus" },
      "targets": [
        {
          "expr": "sum(rate(span_metrics_calls_total{service_name=\"rootstock\", status_code=\"STATUS_CODE_ERROR\"}[1m]))",
          "legendFormat": "errors/s"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 15,
            "lineWidth": 2
          },
          "color": { "mode": "fixed", "fixedColor": "red" }
        },
        "overrides": []
      }
    },
    {
      "id": 3,
      "title": "Request Latency (p50 / p95 / p99)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "datasource": { "uid": "prometheus", "type": "prometheus" },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(span_metrics_duration_milliseconds_bucket{service_name=\"rootstock\"}[1m])) by (le))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(span_metrics_duration_milliseconds_bucket{service_name=\"rootstock\"}[1m])) by (le))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(span_metrics_duration_milliseconds_bucket{service_name=\"rootstock\"}[1m])) by (le))",
          "legendFormat": "p99"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2
          }
        },
        "overrides": []
      }
    },
    {
      "id": 4,
      "title": "Log Volume",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "datasource": { "uid": "loki", "type": "loki" },
      "targets": [
        {
          "expr": "sum(count_over_time({service_name=\"rootstock\"} [1m]))",
          "legendFormat": "logs/min"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 50,
            "lineWidth": 1
          },
          "color": { "mode": "fixed", "fixedColor": "blue" }
        },
        "overrides": []
      }
    },
    {
      "id": 5,
      "title": "Recent Traces",
      "type": "table",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 16 },
      "datasource": { "uid": "tempo", "type": "tempo" },
      "targets": [
        {
          "queryType": "nativeSearch",
          "serviceName": "rootstock",
          "limit": 20
        }
      ]
    },
    {
      "id": 6,
      "title": "Recent Logs",
      "type": "logs",
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 24 },
      "datasource": { "uid": "loki", "type": "loki" },
      "targets": [
        {
          "expr": "{service_name=\"rootstock\"}",
          "refId": "A"
        }
      ],
      "options": {
        "showTime": true,
        "sortOrder": "Descending",
        "enableLogDetails": true,
        "prettifyLogMessage": true
      }
    }
  ],
  "schemaVersion": 39
}
